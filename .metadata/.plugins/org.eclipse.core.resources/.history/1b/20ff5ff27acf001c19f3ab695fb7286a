package service;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import dao.VueloDao;
import model.Vuelo;

@Service
public class VueloServiceImp implements VueloService {

	VueloDao hd;
	
	public VueloServiceImp(@Autowired VueloDao hd) {
		this.hd = hd;
	}
	
	@Override
	public List<Vuelo> findAll() {
		return hd.findAll();
	}

	@Override
	public Vuelo findByNombre(String nombre) {
		return hd.findByNombre(nombre);
	}

}
